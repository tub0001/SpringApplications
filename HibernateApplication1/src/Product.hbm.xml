<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tadigital.entity.Product" table="product">
		<id name="productId" column="product_id">
			<generator class="native"/>
		</id>
		<property name="productName" column="product_name"/>
		<property name="productPrice" column="product_price"/>
		<property name="productQuantity" column="product_quantity"/>
	</class>
</hibernate-mapping>